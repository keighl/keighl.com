%head
  %title {{ page.title }} | Keighl
  %meta{ content: "width=device-width, minimum-scale=1.0, maximum-scale=1.0", name: "viewport" }
  %link{ href: "/assets/stylesheets/screen.css", rel: "stylesheet", type: "text/css"}
  %meta{ name: "description", content: "{{ page.description }}" }
  %link{ rel: "shortcut icon", href: "/favicon.ico" }

  <!--[if lt IE 9]>
  %link{ href: "/assets/stylesheets/lt_ie9.css", rel: "stylesheet", type: "text/css"}
  <![endif]-->

  %script{ src: "/assets/js/modernizr.js" }
%body
  .cover-container
    .cover-surface
      .cover-content
        .cover
          .cover-logo
            .cover-logo-inner
              %a.cover-logo-link{ href: "/" }
                %strong keighl
          %nav.cover-menu
            .cover-menu-inner
              %ul.cover-menu-list
                %li.cover-menu-item
                  %a.cover-menu-link{ href: "/" }
                    home
                    %span.icon-chevron
                %li.cover-menu-item
                  %a.cover-menu-link{ href: "http://clambakeapp.com" }
                    clambake
                    %span.icon-chevron
                %li.cover-menu-item
                  %a.cover-menu-link{ href: "https://github.com/keighl" }
                    github
                    %span.icon-chevron
                %li.cover-menu-item
                  %a.cover-menu-link{ href: "http://twitter.com/keighl" }
                    twitter
                    %span.icon-chevron


  .content-wrapper
    .content-wrapper-inner
      .white-page
        {{ content }}



  .header
    .header-wrapper
      .header-sub-wrapper
        %a.icon-menu.header-menu-icon{ href: "#{}"}
        %a.header-title{ href: "/" }
          %span.text keighl


